[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/var/www/html/project-name
ExecStart=/var/www/html/project-name/env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/var/www/html/project-name/app-name.sock project-name.wsgi:application

[Install]
WantedBy=multi-user.target