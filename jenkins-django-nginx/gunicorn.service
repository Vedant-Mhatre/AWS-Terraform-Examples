[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=project-name
ExecStart=project-name/env/bin/gunicorn --access-logfile - --workers 3 --bind unix:project-name/app-name.sock project-name.wsgi:application

[Install]
WantedBy=multi-user.target